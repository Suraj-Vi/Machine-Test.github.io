@model Machinetest.Context.ProductMaster



@{
    ViewBag.Title = "Product";
}


@using (Html.BeginForm("AddProduct", "Product", FormMethod.Post))
{
    <div class="container">
        <div class="form-group">

            <label>ProductId</label>

            @Html.TextBoxFor(x => x.ProductId, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>ProductName</label>

            @Html.TextAreaFor(x => x.ProductName, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-primary">Submit</button>

        </div>

    </div>
}


