@model Machinetest.Context.CategoryMaster



@{
    ViewBag.Title = "Category";
}


@using (Html.BeginForm("AddCategory", "Category", FormMethod.Post))
{
    <div class="container">
        <div class="form-group">

            <label>CategoryId</label>

            @Html.TextBoxFor(x => x.CategoryId, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>CategoryName</label>

            @Html.TextAreaFor(x => x.CategoryName, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-primary">Submit</button>

        </div>

    </div>
}
